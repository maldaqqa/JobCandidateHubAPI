{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseSettings": {
    "DatabaseType": "InMemory", // Change this value to switch databases (e.g., "SQLite", "MSSQL", "MySQL", "PostgreSQL", "InMemory", "Oracle")
    "ConnectionStrings": {
      "SQLite": "Data Source=JobCandidateHub.db",
      "MSSQL": "Server=myServer;Database=JobCandidateHub;Trusted_Connection=True;",
      "MySQL": "Server=myServer;Database=JobCandidateHub;User=myUser;Password=myPassword;",
      "PostgreSQL": "Host=myServer;Database=JobCandidateHub;Username=myUser;Password=myPassword;",
      "InMemory": "JobCandidateHub",
      "Oracle": "User Id=myUser;Password=myPassword;Data Source=JobCandidateHub"
    }
  },
  "AllowedHosts": "*"
}
